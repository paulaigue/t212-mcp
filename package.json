  "author": "Paul Aigueperse @paulaigue",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paulaigue/t212-mcp.git"
  },
  "bugs": {
    "url": "https://github.com/paulaigue/t212-mcp/issues"
  },
  "homepage": "https://github.com/paulaigue/t212-mcp#readme",
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.8.0",
    "dotenv": "^16.4.7"
  },
  "devDependencies": {
    "@types/node": "^22.13.14",
    "typescript": "^5.8.2"
  }
