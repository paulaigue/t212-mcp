{
  "name": "t212-mcp-server",
  "version": "1.0.0",
  "description": "A Model Context Protocol (MCP) server for interacting with Trading212's API",
  "type": "module",
  "main": "./build/index.js",
  "bin": {
    "t212-mcp": "./build/index.js"
  },
  "scripts": {
    "build": "tsc && chmod 755 build/index.js",
